{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "moduleResolution": "node",
    "pretty": true,
    "baseUrl": "./src",
    "rootDir": "./src",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
